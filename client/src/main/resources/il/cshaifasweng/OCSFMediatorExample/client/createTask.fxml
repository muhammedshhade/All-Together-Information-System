<AnchorPane prefHeight="386.0" prefWidth="339.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.RequestController">
   <children>
      <TextField layoutX="29.0" layoutY="24.0" prefHeight="60.0" prefWidth="276.0" style="-fx-background-color: without;" text="     Submit Service Request">
