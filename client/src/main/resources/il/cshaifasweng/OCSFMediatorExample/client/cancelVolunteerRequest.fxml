<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: brighy;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.CancelVolunteerRequest">
    <children>
        <Label layoutX="108.0" layoutY="32.0" prefHeight="30.0" prefWidth="352.0" text="              Cancel Volunteer Request " textAlignment="CENTER" textFill="#7ad0ea">
            <font>
                <Font name="David Bold" size="20.0" />
            </font>
        </Label>
        <TextArea fx:id="note" layoutX="70.0" layoutY="79.0" prefHeight="42.0" prefWidth="459.0" text="You can cancel volunteer requests that have not been approved or have not &#10;yet been volunteered." />
        <ListView fx:id="requestsList" layoutX="130.0" layoutY="139.0" prefHeight="122.0" prefWidth="308.0" style="-fx-background-color: bright;" />
        <Button fx:id="cancel" layoutX="235.0" layoutY="274.0" mnemonicParsing="false" onAction="#CancelVolunteer" prefHeight="15.0" prefWidth="110.0" style="-fx-background-color: #7ad0ea;" text="Cancel Volunteer" textFill="#fcfcfc">
            <font>
                <Font name="David Bold" size="12.0" />
            </font>
        </Button>
        <Button fx:id="back" layoutX="235.0" layoutY="347.0" mnemonicParsing="false" onAction="#back" prefHeight="15.0" prefWidth="110.0" style="-fx-background-color: #7ad0ea;" text="Back" textFill="WHITE">
            <font>
                <Font name="David Bold" size="12.0" />
            </font>
        </Button>
        <Button fx:id="details" layoutX="235.0" layoutY="310.0" mnemonicParsing="false" onAction="#taskDetails" prefHeight="15.0" prefWidth="110.0" style="-fx-background-color: #7ad0ea;" text="Task datails" textFill="WHITE">
            <font>
                <Font name="David Bold" size="12.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
