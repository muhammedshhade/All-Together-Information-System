<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.CheckRequestService">
    <children>
        <Label alignment="BASELINE_CENTER" layoutX="143.0" layoutY="51.0" prefHeight="36.0" prefWidth="313.0" text="Check Service Requests" textAlignment="CENTER" textFill="#b473ea">
            <font>
                <Font name="David Bold" size="20.0" />
            </font>
        </Label>
        <ListView fx:id="communityTasks" layoutX="45.0" layoutY="113.0" prefHeight="182.0" prefWidth="509.0" style="-fx-background-color: white;" />
        <Button fx:id="back" layoutX="45.0" layoutY="323.0" mnemonicParsing="false" onAction="#previous" prefWidth="110.0" style="-fx-background-color: #b473ea;" text="Back" textFill="WHITE">
         <font>
            <Font name="David Bold" size="14.0" />
         </font></Button>
        <Button fx:id="tasks" layoutX="430.0" layoutY="323.0" mnemonicParsing="false" onAction="#tasksDetails" prefWidth="110.0" style="-fx-background-color: #b473ea;" text="Task details" textFill="WHITE">
         <font>
            <Font name="David Bold" size="14.0" />
         </font></Button>
        <Button fx:id="acceptbtn" layoutX="177.0" layoutY="323.0" mnemonicParsing="false" onAction="#accept" prefHeight="26.0" prefWidth="110.0" style="-fx-background-color: #b473ea;" text="Accept" textFill="WHITE">
         <font>
            <Font name="David Bold" size="14.0" />
         </font></Button>
        <Button fx:id="removebtn" layoutX="300.0" layoutY="323.0" mnemonicParsing="false" onAction="#remove" prefHeight="26.0" prefWidth="110.0" style="-fx-background-color: #b473ea;" text="Reject" textFill="WHITE">
         <font>
            <Font name="David Bold" size="14.0" />
         </font></Button>
      <ImageView fx:id="im" fitHeight="92.0" fitWidth="122.0" layoutX="220.0" layoutY="424.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="refresh" layoutX="232.0" layoutY="369.0" mnemonicParsing="false" onAction="#Refresh" prefWidth="110.0" style="-fx-background-color: #b473ea;" text="Refresh" textFill="WHITE">
         <font>
            <Font name="David Bold" size="14.0" />
         </font>
      </Button>
    </children>
</AnchorPane>
